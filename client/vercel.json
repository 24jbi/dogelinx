{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": false,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "https://dogelinx-backend.onrender.com/api/$1",
      "headers": {
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "^/avatar items/(.*)",
      "dest": "/public/avatar items/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
